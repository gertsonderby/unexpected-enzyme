    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.2.0",
    "mocha": "^6.1.4",
    "prettier": "1.18.2",
    "react": "16",
    "react-dom": "16",
    "react-element-to-jsx-string": "^14.0.3",
    "read-pkg-up": "^6.0.0",
    "sinon": "^7.3.2",
    "unexpected": "^11.6.1"
  },
